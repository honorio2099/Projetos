@model IEnumerable<DicasTarefa.Models.Professores>

@{
    ViewBag.Title = "Buscar";
}

<h2>Buscar</h2>


@foreach (var item in Model)
{
    <div class="panel panel-default">
        <div class="panel-heading">
            <p class="panel-title">@item.NomeProfessor</p>
        </div>
        <div class="panel-body">
            <p>@item.Disciplinas</p>
            <p>@item.Experiencia</p>
        </div>
        <div class="panel-footer">
            @Html.ActionLink("Ler mais", "Detalhes", "Professores", new { id = @item.IdProfessor }, null) 
        </div>
    </div>
}
